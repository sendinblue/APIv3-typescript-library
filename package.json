    },
    "devDependencies": {
        "@types/bluebird": "*",
        "@types/node": "*",
        "@types/request": "*",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "eslint": "^7.23.0",
        "eslint-plugin-unused-imports": "^1.1.0",
        "minimist": "^1.2.5",
        "rimraf": "^3.0.2",
        "typescript": "^5.1.6"
